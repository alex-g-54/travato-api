<p>Find me in app/views/itineraries/new.html.erb</p>

<%= form_for @itinerary do |f| %>
  <div>
    <%= f.label(:name, "Itinerary Name") %>
    <%= f.text_field :name) %>
  </div>

  <div>
    <%= f.label(:city, "City") %>
    <%= f.text_field(:city) %>
  </div>

  <div>
    <%= f.label(:price, "Price") %>
    <%= f.text_field(:price) %>
  </div>

  <div>
    <%= f.label(:description, "Description") %>
    <%= f.text_area(:description, :text, rows: 20, cols: 40) %>
  </div>

  <div>
    <%= f.label(:total_capacity, "Total Capacity") %>
    <%= f.text_field(:total_capacity) %>
  </div>

  <div>
    <%= f.label(:pic_url, "Pic URL") %>
    <%= f.text_field(:pic_url) %>
  </div>

  <p>
    <%= f.label :start_time, "Start Time" %>
    <%= f.date_field Date.today, :start_time %>
  </p>

  <p>
    <%= f.label :end_time, "End Time" %>
    <%= f.date_field Date.tomorrow, :end_time %>
  </p>

  <%= submit_tag("Create Itinerary") %>
<% end %>